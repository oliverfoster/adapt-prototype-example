{
	"$ref": "schemas/text.json",
	"title": "GMCQ",
	"id": "gmcq",
	"properties": {
		"_attempts": {
			"type": "number"
		},
		"_questionWeight": {
			"type": "number"
		},
		"_isRandom": {
			"type": "boolean"
		},
		"_selectable": {
			"type": "number"
		},
		"_items": {
		    "type": "array",
			"format": "tabs",
			"title": "_items",
			"options": {
				"collapsed": true
			},
			"uniqueItems": false,
			"items": {
				"type": "object",
				"headerTemplate": "{{ i0 }}",
				"properties": {
					"text": {
						"type": "string"
					},
					"_shouldBeSelected": {
						"type": "boolean"
					},
					"_graphic": {
						"type": "object",
						"options": {
							"collapsed": true
						},
						"name": "_graphic",
						"properties": {
							"large": {
								"type": "string"
							},
							"alt": {
								"type": "string"
							},
							"small": {
								"type": "string"
							}
						}
					}
				}
			}
		},
		"_feedback": {
			"type": "object",
			"options": {
				"collapsed": true
			},
			"properties": {
				"correct": {
					"type": "string"
				},
				"_incorrect": {
					"type": "object",
					"properties": {
						"final": {
							"type": "string"
						},
						"notfinal": {
							"type": "string"
						}
					}
				},
				"_partlyCorrect": {
					"type": "object",
					"properties": {
						"final": {
							"type": "string"
						},
						"notfinal": {
							"type": "string"
						}
					}
				}
			}
		}
  	}
}